<?xml version="1.0"?>
<package format="3">
  <name>lab_manipulator</name>
  <version>0.0.1</version>
  <description>ROB6 Lab assignment Manipulator</description>

  <maintainer email="slaurs24@student.aau.dk">Soren</maintainer>
  <license>Apache-2.0</license>

  <!-- ROS 2 dependencies -->
  <buildtool_depend>ament_cmake</buildtool_depend>
  <depend>rclcpp</depend>
  <depend>ament_index_cpp</depend>
  <depend>tinyxml</depend>
